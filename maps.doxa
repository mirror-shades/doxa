struct Person {
    name :: string,
    age :: int,
}

var mike is Person {
    name is "Mike",
    age is 25,
}

var bob is Person {
    name is "Bob",
    age is 30,
}

var map MyMap {
    1 is mike,
    2 is bob,
}

MyMap[1]? // Mike
MyMap[1].name? // Mike
MyMap[1].age? // 25
MyMap[2]? // Bob
MyMap[2].name? // Bob
MyMap[2].age? // 30
