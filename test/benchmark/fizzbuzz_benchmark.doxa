function fizzbuzz(x :: int) {
    var current is x
    const end is 1000000
    while current <= end {
        if current % 15 equals 0 then "fizzbuzz"
        else if current % 3 equals 0 then "fizz"
        else if current % 5 equals 0 then "buzz"
        else current
        
        current += 1
    }
}

@print("Starting Fizzbuzz test\n")
const startTime is @tick()
fizzbuzz(1)
const endTime is @tick()
@print("FizzBuzz time: {(endTime - startTime) / 1000000000 }s\n")