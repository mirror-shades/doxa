module g from "graphics"

entry function main() {
    const rl is g.raylib
    const dg is g.doxa
    
    const w is 800
    const h is 600
    const fps is 60
    const name is "Doxa + Raylib"
    
    var app is dg.Init(w, h, fps, name)
    
    while app.running {
        dg.Draw()
        rl.ClearBackground(rl.ORANGE)
    }
}

