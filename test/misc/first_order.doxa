// AND
(true ∧ true)?; // true
(true ∧ false)?; // false
(false ∧ true)?; // false
(false ∧ false)?; // false

// OR
(true ∨ false)?; // true 
(false ∨ true)?; // true
(false ∨ false)?; // false

// NAND
(true ↑ true)?; // false
(true ↑ false)?; // true
(false ↑ true)?; // true
(false ↑ false)?; // true

// XOR
(true ⊕ true)?; // false
(true ⊕ false)?; // true
(false ⊕ true)?; // true
(false ⊕ false)?; // false

// NOR
(true ↓ true)?; // false
(true ↓ false)?; // false
(false ↓ true)?; // false
(false ↓ false)?; // true

// IMPLIES
(true → true)?; // true
(true → false)?; // false 
(false → true)?; // true
(false → false)?; // true

// EQUIVALENCE
(true ↔ false)?; // false
(false ↔ true)?; // false
(false ↔ false)?; // true
(true ↔ true)?; // true

// NOT
(¬ true)?; // false
(¬ false)?; // true
