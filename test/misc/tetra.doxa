fn doorPositionTetra(isOpen :: tetra) {
    if (isOpen == true and isOpen == false) then "ajar"?;
    else if (isOpen == true) then "open"?;
    else if (isOpen == false) then "closed"?;
    else "neither"?;
}

doorPositionTetra(both); // ajar
doorPositionTetra(true); // open
doorPositionTetra(false); // closed
doorPositionTetra(neither); // neither