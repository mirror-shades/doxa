// Testing multiple specific imports: "import add, subtract from "./misc/extended_math.doxa";"
// This would import multiple functions directly into the current scope

// For now, we'll create a simple example with the available math module
import add from "./misc/math.doxa";

// Note: This demonstrates the syntax design
// In a full implementation, you could do:
// import add, subtract, multiply from "./misc/extended_math.doxa";

pub function testMultipleImports(x :: int, y :: int, z :: int) returns(int) {
    // Using the imported function directly
    const step1 is add(x, y);
    const step2 is add(step1, z);
    
    // In the future with multiple imports, you could also use:
    // const step3 is subtract(step2, 1);
    // const step4 is multiply(step3, 2);
    
    return step2;
}

pub function demonstrateMultiImportPattern() returns(int) {
    // Example of how multiple imports would work
    return testMultipleImports(10, 20, 30); // Should return 60
} 