// Basic arithmetic operations
var expr1 is 5 + 10 * 2      // 25 (multiplication before addition)
var expr2 is (5 + 10) * 2    // 30 (parentheses change precedence)

// Integer division (truncates)
var expr3 is 20 / 4 + 3 * 2  // 11 (integer division)
var expr4 is (20 / 4 + 3) * 2  // 16 (integer division)

// Force float division
var expr5 is 20.0 / 4 + 3 * 2  // 11.0 (float division)
var expr6 is (20.0 / 4 + 3) * 2  // 16.0 (float division)

// With variables
var a is 5
var b is 10
var expr7 is a * b + a / b   // 50 (integer division: 50 + 0 = 50)
var expr8 is (a + b) * (a - b) // -75 (15 * -5)

// Explicit float conversion
var expr9 is (a * 1.0) / b   // 0.5 (float division)

// Print results
expr1?
expr2?
expr3?
expr4?
expr5?
expr6?
expr7?
expr8?
expr9?
