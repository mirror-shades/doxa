// Testing specific import syntax
import add from "./test/misc/math.doxa";

const limit :: int is 255;

pub function safeAddSpecific(a :: int, b :: int) returns(int) {
    if(a > limit or b > limit) then {
        "Overflow"?;
        return -1;
    } 
    
    if(a < 0 or b < 0) then {
        "Underflow"?;
        return -1;
    }

    // Using specific import: add(a, b) directly (no namespace needed)
    return add(a, b);
} 